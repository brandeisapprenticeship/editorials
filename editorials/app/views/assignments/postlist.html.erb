<h1>Posts for Assignment <%=@assignment.title%></h1>
<table class='table table-bordered'>
  <thead>
      <tr>
        <th>Headline</th>
        <th>Author</th>
      </tr>
    </thead>
    <tbody>
      <% @posts.each do |post| %>
        <tr>
          <td><%= link_to post.title, post %></td>
          <td><%= post.user.name%></td>
        </tr>
      <% end %>
    </tbody>
</table>